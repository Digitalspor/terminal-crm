{
  "name": "crm-terminal",
  "version": "0.1.0",
  "description": "Terminal-basert CRM med Claude Canvas og Fiken integrasjon",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "crm": "node src/index.js",
    "dashboard": "node src/index.js dashboard",
    "dashboard:dev": "nodemon --stdin src/index.js dashboard",
    "dashboard:watch": "node --watch src/index.js dashboard",
    "dev": "node --watch src/index.js",
    "commit": "git add . && git commit",
    "sync": "git pull --rebase && git push",
    "pull": "git pull --rebase"
  },
  "keywords": [
    "crm",
    "terminal",
    "cli",
    "fiken"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.5",
    "blessed": "^0.1.81",
    "blessed-contrib": "^4.11.0",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "date-fns": "^3.0.6",
    "dotenv": "^16.3.1",
    "googleapis": "^131.0.0",
    "inquirer": "^9.2.12",
    "ora": "^8.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.1.11"
  }
}
